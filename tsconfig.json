{
  "compilerOptions": {
   
    "target": "es5",   
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
 
    "strict": true,
   "skipLibCheck": true
  }
}
